{
  "name": "Hello Bookmarks",
  "version": "1.0",
  "description": "Bookmarks Demo",
  "permissions": [
    "bookmarks"
  ],
  "browser_action": {
      "default_title": "Hello Bookmarks",
      "default_icon": "logo.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
